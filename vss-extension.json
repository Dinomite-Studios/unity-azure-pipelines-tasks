{
    "manifestVersion": 1,
    "id": "64e90d50-a9c0-11e8-a356-d3eab7857116",
    "version": "2.4.1",
    "name": "Unity Tools for Azure DevOps",
    "description": "The Unity Tools for Azure DevOps adds tasks to easily enable build and release pipelines for Unity 3D projects.",
    "publisher": "DinomiteStudios",
    "public": true,
    "icons": {
        "default": "images/extensionIcon.png"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Build and release"
    ],
    "tags": [
        "Unity",
        "Build",
        "UWP",
        "Android",
        "iOS",
        "3D",
        "tvOS",
        "WebGL",
        "CI",
        "CD"
    ],
    "links": {
        "getstarted": {
            "uri": "https://github.com/Dinomite-Studios/unity-azure-pipelines-tasks/wiki"
        },
        "license": {
            "uri": "https://github.com/Dinomite-Studios/unity-azure-pipelines-tasks/blob/master/LICENSE"
        },
        "support": {
            "uri": "https://github.com/Dinomite-Studios/unity-azure-pipelines-tasks/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/Dinomite-Studios/unity-azure-pipelines-tasks"
    },
    "files": [
        {
            "path": "images",
            "addressable": true
        },
        {
            "path": "Tasks/UnityBuild/UnityBuildV1"
        },
        {
            "path": "Tasks/UnityBuild/UnityBuildV2"
        },
        {
            "path": "Tasks/UnityGetProjectVersion/UnityGetProjectVersionV1"
        },
        {
            "path": "Tasks/UnityActivateLicense/UnityActivateLicenseV1"
        }
    ],
    "branding": {
        "color": "rgb(0, 0, 0)",
        "theme": "dark"
    },
    "screenshots": [
        {
            "path": "images/screen1.PNG"
        },
        {
            "path": "images/screen2.PNG"
        },
        {
            "path": "images/screen3.PNG"
        },
        {
            "path": "images/screen4.PNG"
        }
    ],
    "contributions": [
        {
            "id": "custom-unity-build-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/UnityBuild"
            }
        },
        {
            "id": "custom-unity-get-project-version-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/UnityGetProjectVersion"
            }
        },
        {
            "id": "custom-unity-activate-license-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/UnityActivateLicense"
            }
        }
    ]
}
