{
  "name": "@dinomite-studios/unity-install-task",
  "version": "1.0.0",
  "description": "Azure Pipelines task to install Unity editors and modules on the executing machine.",
  "main": "unity-install.js",
  "scripts": {
    "build": "tsc",
    "upload": "tfx build tasks upload --task-path ./",
    "dev-delete": "tfx build tasks delete --task-id de4be6e2-9c1e-4ae3-9a7e-6854f719bcc3",
    "dev-upload": "npm run build && npm run upload"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dinomite-Studios/unity-azure-pipelines-tasks"
  },
  "author": "Dinomite Studios",
  "license": "MIT",
  "dependencies": {
    "@dinomite-studios/unity-azure-pipelines-tasks-lib": "^1.0.0",
    "azure-pipelines-task-lib": "^4.1.0"
  },
  "devDependencies": {
    "typescript": "^4.7.4",
    "@types/node": "^18.0.0",
    "@types/q": "^1.5.5"
  }
}
