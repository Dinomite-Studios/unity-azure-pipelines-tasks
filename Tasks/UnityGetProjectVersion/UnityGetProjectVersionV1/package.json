  "dependencies": {
    "azure-pipelines-task-lib": "^2.12.0",
    "@dinomite-studios/unity-project-version": "^1.1.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.19",
    "@types/q": "^1.5.4",
    "typescript": "^4.1.3"
  }
}
